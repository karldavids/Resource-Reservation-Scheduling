<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="Slot" />
  <meta>
    <versionId value="3" />
    <lastUpdated value="2020-05-29T08:22:36.5363717+00:00" />
  </meta>
  <url value="http://StructureDefinition.hl7fhir.cn/Slot" />
  <name value="Slot" />
  <title value="号源时段" />
  <status value="draft" />
  <description value="号源时段，用于描述预约的医疗服务执行的始止时间。和预约相关的其它信息可通过引用班次资源来描述。" />
  <fhirVersion value="4.0.1" />
  <mapping>
    <identity value="rim" />
    <uri value="http://hl7.org/v3" />
    <name value="RIM Mapping" />
  </mapping>
  <mapping>
    <identity value="ical" />
    <uri value="http://ietf.org/rfc/2445" />
    <name value="iCalendar" />
  </mapping>
  <mapping>
    <identity value="w5" />
    <uri value="http://hl7.org/fhir/fivews" />
    <name value="FiveWs Pattern Mapping" />
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Slot" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Slot" />
  <derivation value="constraint" />
  <differential>
    <element id="Slot">
      <path value="Slot" />
      <short value="号源时段，用于描述一段可预约的医疗服务的始止时间" />
      <definition value="号源时段，用于描述一段可预约的医疗服务的始止时间。若要表示和预约相关的其它信息可引用班次资源。" />
    </element>
    <element id="Slot.identifier">
      <path value="Slot.identifier" />
      <short value="号源时段外部ID" />
      <definition value="描述号源时段的外部ID" />
      <type>
        <code value="Identifier" />
        <profile value="http://StructureDefinition.hl7fhir.cn/Identifier" />
      </type>
    </element>
    <element id="Slot.serviceCategory">
      <path value="Slot.serviceCategory" />
      <short value="服务类别" />
      <definition value="描述该班次中提供的服务所属的类别，如：医疗服务，康养服务，美容服务，儿童保健、老年护理等。" />
      <type>
        <code value="CodeableConcept" />
        <profile value="http://StructureDefinition.hl7fhir.cn/CodeableConcept" />
      </type>
    </element>
    <element id="Slot.serviceType">
      <path value="Slot.serviceType" />
      <short value="服务项目分类" />
      <definition value="将在该号源时段内执行的具体服务。如：门诊预约挂号、检查预约、手术预约等。" />
      <type>
        <code value="CodeableConcept" />
        <profile value="http://StructureDefinition.hl7fhir.cn/CodeableConcept" />
      </type>
    </element>
    <element id="Slot.specialty">
      <path value="Slot.specialty" />
      <short value="专业要求" />
      <definition value="在此预约号源中执行的医疗服务者，的临床专业要求，如 骨科门诊、牙科门诊、心内科门诊。" />
      <type>
        <code value="CodeableConcept" />
        <profile value="http://StructureDefinition.hl7fhir.cn/CodeableConcept" />
      </type>
    </element>
    <element id="Slot.appointmentType">
      <path value="Slot.appointmentType" />
      <short value="预约患者的类型" />
      <definition value="描述此号源提供的预约方式或此号源可供哪类患者预约。如：检查预约、随访预约、紧急预约、常规预约、例行约会" />
      <type>
        <code value="CodeableConcept" />
        <profile value="http://StructureDefinition.hl7fhir.cn/CodeableConcept" />
      </type>
    </element>
    <element id="Slot.schedule">
      <path value="Slot.schedule" />
      <short value="所属班次" />
      <definition value="通过引用班次资源来描述和预约相关的其它信息，如排班信息、需占用的医疗资源等" />
      <type>
        <code value="Reference" />
        <profile value="http://StructureDefinition.hl7fhir.cn/Reference" />
        <targetProfile value="http://StructureDefinition.hl7fhir.cn/Schedule" />
      </type>
    </element>
    <element id="Slot.status">
      <path value="Slot.status" />
      <short value="号源状态" />
      <definition value="用编码形式表示号源的当前状态。包括：繁忙|空闲|繁忙（不可用）|繁忙（暂定的预约）|录入错误。" />
    </element>
    <element id="Slot.start">
      <path value="Slot.start" />
      <short value="开始时间" />
      <definition value="此号源提供的医疗服务的开始时间，该时间应晚于所属班次的开始时间。" />
    </element>
    <element id="Slot.end">
      <path value="Slot.end" />
      <short value="结束时间" />
      <definition value="此号源提供的医疗服务的结束时间。" />
    </element>
    <element id="Slot.overbooked">
      <path value="Slot.overbooked" />
      <short value="是否被超额预定" />
      <definition value="如果已被超额预定，则可再接受预约。" />
    </element>
    <element id="Slot.comment">
      <path value="Slot.comment" />
      <short value="备注" />
      <definition value="备注信息，内容不能大于1M" />
    </element>
  </differential>
</StructureDefinition>